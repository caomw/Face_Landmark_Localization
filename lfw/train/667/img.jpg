function is_device(){
        var user_agent = navigator.userAgent;
        var types = new Array("iPhone", "iPod", "iPad", "Android", "RIM", "BlackBerry", "Windows");
	var mobiles = new Array("Mobile", "Windows Phone", "Windows CE");
        var i, j, position, pos_mobile;
        for (i=0; i<types.length; i++){
                position = user_agent.lastIndexOf(types[i]);
                if(position >= 0){
			if(types[i] == "Android" || types[i] == "Windows"){
				for(j=0; j<mobiles.length; j++){
					pos_mobile = user_agent.lastIndexOf(mobiles[j]);
					if(pos_mobile >=0)  return types[i].toLowerCase() + mobiles[j].toLowerCase();
				}
			}
			if(types[i] == "Windows"){
				return 'www';
			}else{
				if( (types[i] == "Android") || (types[i] == "RIM") )	return types[i].toLowerCase() + "tablet";
				else	return types[i].toLowerCase();
			}
		}
        }
        return 'www';
}
