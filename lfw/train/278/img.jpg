					rolarMenu('esquerda');
				}	
			}).mouseout(function(){
				scrollAtivadoEsquerda = false;
			});
			
			function rolarMenu(direcao){
	
				if(direcao == "direita" && scrollAtivadoDireita){
	
					$("#mainScroll").scrollLeft($("#mainScroll").scrollLeft() + 10);
					setTimeout(function(){ rolarMenu(direcao); }, 100);
	
				}else if(direcao == "esquerda" && scrollAtivadoEsquerda){
					$("#mainScroll").scrollLeft($("#mainScroll").scrollLeft()- 10);
