		$("#mainAssuntosLista").css("width", tamanhoMenuScroll+"px");

		//posicao do li com hover
		$("ul.menu li.submenu").hover(function(){

			var tamanhoMenu = 0;

			//adiciona o efeito de hover
			var thisoffset = $(this).offset().left;
		
			$(this).addClass("submenuHover");
			
			$(this).find("ul").each(function(){
				
				$(this).show();
				
				$(this).css("width", 9999); /* very important */	
							
				$(this).find("li").each(function(){
						tamanhoMenu = tamanhoMenu + $(this).width() + 17;
				});
				
				$(this).hide();
				
				$(this).css("width", tamanhoMenu);
				
				$(this).addClass("submenuHoverUl");	
				
				//define posicao
				if(thisoffset + tamanhoMenu > 1000){
					$(this).addClass("direita");
					$(this).css("left",(tamanhoMenu * -1) - 2);
				}else{
					$(this).removeClass("direita");
					$(this).css("left", -1);
				}

				$(this).show();
			});

			
		}, function(){
			$(this).removeClass("submenuHover");
			
			$(this).find("ul").each(function(){
				$(this).removeClass("submenuHoverUl");
				$(this).hide();
			});
		});
		
		if(tamanhoMenuScroll > 990) {
		
