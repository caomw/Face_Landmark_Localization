  function parseUrlAndReplaceCurrentUrl()
  {
  		 var isUrlChanged= false;
     	 var url = window.location.href;
    	 if(url.indexOf("?") != -1){
         	url = url.replace("?", "&");
    		isUrlChanged= true;
    	 }
    	 if(url.indexOf(".com/video/") != -1){
    		url = url.replace(".com/video/", ".com/#/video/");
    		isUrlChanged= true;
    	 }
    	 if( isUrlChanged== true)
    	 {
    	 	window.location.assign(url);
    	 }	 
  }	
 
